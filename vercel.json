{
  "version": 2,
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(images|styles|js|uploads)/(.*)", "dest": "/public/$1/$2", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/favicon\\.ico", "dest": "/public/images/Logo.png" },
    { "src": "/favicon\\.png", "dest": "/public/images/Logo.png" },
    { "src": "/(.*)", "dest": "/api/server.js" }
  ]
}
